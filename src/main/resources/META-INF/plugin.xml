<idea-plugin>
    <id>com.wangjin.doc.gen-doc-plugin</id>
    <name>文档同步工具</name>
    <vendor email="wajncn@gmail.com" url="https://javanet123.com/">王进</vendor>
    <description><![CDATA[
    文档同步工具.<br>
    <em>
    第一交付中心第四部门提供<br>
    项目首次执行会在根目录生成配置文件.<br>
    Tools->生成文档.<br>
    </em>
    ]]></description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
<!--    <depends>com.intellij.modules.ultimate</depends>-->
    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="MainPlugin" class="com.wangjin.doc.MainPlugin" text="生成文档" description="MainPlugin">
            <add-to-group group-id="ToolsMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>